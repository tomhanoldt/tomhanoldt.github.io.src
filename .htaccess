RewriteEngine on

# www.tomhanoldt.info
# force www
RewriteCond %{HTTP_HOST} ^tomhanoldt\.info [NC]
RewriteRule ^(.*)$ https://www.tomhanoldt.info/$1 [L,R=301,NC]

# rewrite for main domain
RewriteCond %{HTTP_HOST} ^www.tomhanoldt.info$
RewriteCond %{REQUEST_URI} !^/www.tomhanoldt.info/
RewriteRule ^(.*)$ /www.tomhanoldt.info/$1 [NC,L]
